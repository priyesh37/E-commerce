<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1" import="vo.*" import="dao.productsdao" import="java.util.*"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>A-SHOP</title>
	<link rel="stylesheet" href="css/common.css">
	<%@include file="common/cdn.jsp"%>
</head>
<body>

	<%@include file="common/header.jsp"%>
		
	<h1>Welcome!You get a right place to shop</h1>
			
	<br>
	<br>
	
		
		<div class="container">
			<div class="row">
				<div class="col-sm-3">
					<img alt="not found" class="pimg" src="data:image/jpeg;base64,">
				</div>
				<div class="col-sm-3">
					<img alt="not found" class="pimg" src="data:image/jpeg;base64,">
				</div>
				<div class="col-sm-3">
					<img alt="not found" class="pimg" src="data:image/jpeg;base64,">
				</div>
				<div class="col-sm-3">
					<img alt="not found" class="pimg" src="data:image/jpeg;base64,">
				</div>
			</div>
			<div class="row">
								
				<%
				productsdao d2=new productsdao();
				products p1=new products();
				
				
				List<products> ls= d2.getAll(p1);
				
				//HttpSession session= request.getSession();
				session.setAttribute("list_product", ls);
				%>
				<%@ taglib prefix="x" uri="http://java.sun.com/jstl/core_rt" %>
					
						<x:forEach items="${sessionScope.list_product}" var="i">
						 
	
					<div class="col-sm-3">
						<div class="product">
							
							<div class="title" style="color:darkblue">Title : ${i.getTitle()}</div>
							<div class="cost">Cost : ${i.getCost()}</div>
							<div class="quantity">Quantity : ${i.quantity}</div>
							<div class="cost">Description : ${i.getDescription()}</div>	
							
						</div>
						<div class="action">
							<button class="btn btn-primary" onclick="addtocart(${i.getId()})">Add TO Cart</button>
							<button class="btn btn-success">Buy Now</button>
						</div>
					</div>
						</x:forEach> 
						 
				
			</div>
			
		</div>
		
	<%@include file="common/footer.jsp"%>
			<script type="text/javascript">
					function addtocart(	id){
						
					$.post("addtocart",{
						id: id
					},function(data,status){
						$(".stickycart").text(data+"Items");
					});
				}
			</script>
			<div class="stickycart" onclick="window.location.href='cart-item.jsp'">
			<%	if(session.getAttribute("addtocart") !=null){
				out.print(((List)session.getAttribute("addtocart")).size()+"Items");
			}
		
			%>	
		</div>
		
</body>
</html>
